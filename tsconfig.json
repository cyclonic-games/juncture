{
    "compilerOptions": {
        "baseUrl": ".",
        "downlevelIteration": true,
        "lib": [
            "dom",
            "esnext"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "paths": {
            "*": [ "*" ]
        },
        "preserveConstEnums": true,
        "removeComments": true,
        "target": "es6"
    },
    "include": [
        "**/*"
    ]
}
